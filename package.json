{
  "name": "MarvelComicExplorer",
  "version": "0.3.0",
  "description": "A great way to browse through the insanely convoluted marvel comic universe!",
  "engines": {
    "node": "12.x"
  },
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "dev:client": "rollup -c -w",
    "start:client": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "dev:server": "PORT=3000 node_modules/.bin/nodemon server.js",
    "test:server": "node test.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.15.2",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4",
    "request": "^2.81.0",
    "sirv-cli": "^0.4.4"
  },
  "devDependencies": {
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "tape": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TimmermanDave/MarvelComicExplorer"
  },
  "keywords": [
    "node",
    "heroku",
    "express",
    "svelte",
    "rollup"
  ],
  "license": "MIT"
}
